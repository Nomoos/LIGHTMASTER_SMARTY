<div class="sidebar" id="sidebar">    <script type="text/javascript">        try {            ace.settings.check('sidebar', 'fixed')        } catch (e) {        }    </script>    {*    <div class="sidebar-shortcuts" id="sidebar-shortcuts">    <div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">    <button class="btn btn-success">    <i class="icon-signal"></i>    </button>    <button class="btn btn-info">    <i class="icon-pencil"></i>    </button>    <button class="btn btn-warning">    <i class="icon-group"></i>    </button>    <button class="btn btn-danger">    <i class="icon-cogs"></i>    </button>    </div>    <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">    <span class="btn btn-success"></span>    <span class="btn btn-info"></span>    <span class="btn btn-warning"></span>    <span class="btn btn-danger"></span>    </div>    </div><!-- #sidebar-shortcuts -->    *}    <ul class="nav nav-list">        {foreach $modul AS $key=>$value}            {if $value.zobraz==2}                <li{if $smarty.get.page==$value.page} class="active open"{/if}>                    <a href="#" class="dropdown-toggle">                        <i class="{$value.icon|default:'icon-circle'}"></i>                        <span class="menu-text {if $value.zobraz=='0'}red{/if}"> {$value.nazev}</span>                        <b class="arrow fa fa-angle-down"></b>                    </a>                    {if $value.sub}                        <ul class="submenu">                            {foreach $value.sub AS $k=>$v}                                {if $v.zobraz==2}                                    <li{if $smarty.get.page==$value.page AND $smarty.get.subpage==$v.page} class="active"{/if}>                                        <a href="{if $v.url}{$setup.adm.www}{$v.url}{else}#{/if}">                                            <i class="{$v.icon|default:'icon-circle'}"></i>                                            {$v.nazev}                                        </a>                                    </li>                                {/if}                            {/foreach}                        </ul>                    {/if}                </li>            {/if}        {/foreach}    </ul><!-- /.nav-list -->    <div class="sidebar-collapse" id="sidebar-collapse">        <i class="icon-double-angle-left" data-icon1="icon-double-angle-left" data-icon2="icon-double-angle-right"></i>    </div>    <script type="text/javascript">        try {            ace.settings.check('sidebar', 'collapsed')        } catch (e) {        }    </script></div>