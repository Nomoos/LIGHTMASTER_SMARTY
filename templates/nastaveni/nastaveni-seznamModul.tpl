<div class="dd dd-draghandle" id="nestable">    <ol class="dd-list">        {foreach $list AS $it}            <li class="dd-item dd2-item" data-id="{$it.id}">                <div class="dd-handle dd2-handle" title="Přesunout">                    <i class="normal-icon {$it.icon|default:'icon-circle'} bigger-130"></i>                    <i class="drag-icon icon-move bigger-125"></i>                </div>                <div class="dd2-content {if $it.zobraz=='0'}red{/if}">                    {$it.nazev} <span class="lighter grey">&nbsp;{$it.page}</span>                    <div class="pull-right action-buttons">                        <a class="" href="{$setup.adm.www}nastaveni/editModul.html?id={$it.id}" title="Editovat">                            <i class="icon-pencil bigger-130"></i>                        </a>                    </div>                </div>                {if $it.submodul}                    <ol class="dd-list">                        {foreach $it.submodul AS $it2}                            <li class="dd-item dd2-item" data-id="{$it2.id}">                                <div class="dd-handle dd2-handle" title="Přesunout">                                    <i class="normal-icon {$it2.icon|default:'icon-circle'} bigger-130"></i>                                    <i class="drag-icon icon-move bigger-125"></i>                                </div>                                <div class="dd2-content {if $it2.zobraz=='0'}red{/if}">                                    {$it2.nazev} <span class="lighter grey">&nbsp;{$it2.page}</span>                                    <div class="pull-right action-buttons">                                        <a class="blue" href="{$setup.adm.www}nastaveni/editModul.html?id={$it2.id}" title="Editovat">                                            <i class="icon-pencil bigger-130"></i>                                        </a>                                    </div>                                </div>                            </li>                        {/foreach}                    </ol>                {/if}            </li>        {/foreach}    </ol></div>