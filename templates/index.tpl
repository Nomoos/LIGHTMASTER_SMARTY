adm.www}assets/css/ace-ie.min.css" />        <![endif]-->        <!-- inline styles related to this page -->        <link rel="stylesheet" href="{$setup.adm.www}assets/css/datepicker.css" />        <!-- ace settings handler -->        <script src="{$setup.adm.www}assets/js/ace-extra.min.js"></script>        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->        <!--[if lt IE 9]>        <script src="{$setup.adm.www}assets/js/html5shiv.js"></script>        <script src="{$setup.adm.www}assets/js/respond.min.js"></script>        <![endif]-->        <!-- basic scripts -->        <!--[if !IE]> -->        <script type="text/javascript">            window.jQuery || document.write("<script src='{$setup.adm.www}assets/js/jquery-2.0.3.min.js'>" + "<" + "/script>");        </script>        <!-- <![endif]-->        <!--[if IE]>        <script type="text/javascript">            window.jQuery || document.write("<script src='{$setup.adm.www}assets/js/jquery-1.10.2.min.js'>"+"<"+"/script>");        </script>        <![endif]-->        <!-- page specific plugin scripts -->        <script src="{$setup.adm.www}assets/js/jquery-ui-1.10.3.full.min.js"></script>        <script src="{$setup.adm.www}assets/js/jquery.ui.touch-punch.min.js"></script>        <!-- romanovo js  -->        <script src="{$setup.adm.www}assets/js/chosen.jquery.min.js"></script>        <script src="{$setup.adm.www}assets/js/leaflet.js"></script>        <script src='https://api.mapbox.com/mapbox.js/plugins/leaflet-draw/v0.2.2/leaflet.draw.js'></script>        <!-- romanovo css  -->        <link rel="stylesheet" href="{$setup.adm.www}assets/css/leaflet.css"/>        <link href='https://api.mapbox.com/mapbox.js/plugins/leaflet-draw/v0.2.2/leaflet.draw.css' rel='stylesheet'/>    </head>    <body>        {include "./includes/ace-navbar.tpl"}        <div class="main-container" id="main-container">            <script type="text/javascript">                try {                    ace.settings.check('main-container', 'fixed')                } catch (e) {                }            </script>            <div class="main-container-inner">                <a class="menu-toggler" id="menu-toggler" href="#">                    <span class="menu-text"></span>                </a>                {include "./includes/ace-sidebar.tpl"}                <div class="main-content">                    <div class="breadcrumbs" id="breadcrumbs">                        <script type="text/javascript">                            try {                                ace.settings.check('breadcrumbs', 'fixed')                            } catch (e) {                            }                        </script>                        <ul class="breadcrumb">                            <li>                                <i class="icon-home home-icon"></i>                                <a href="#">Domů</a>                            </li>                            {if $smarty.get.page}                                <li><a href="#">{$modul[$smarty.get.page].nazev}</a></li>                                {/if}                                {if $smarty.get.subpage}                                <li class="active">{$modul[$smarty.get.page].sub[$smarty.get.subpage].nazev}</li>                                {/if}                        </ul><!-- .breadcrumb -->                        {if $navSearch.show}                        <div class="nav-search" id="nav-search">                            <form action="{$setup.request_url}" class="form-search" method="get">                                <span class="input-icon">                                    <input type="text" name="search" placeholder="Hledat ..." class="nav-search-input width-100" id="search" autocomplete="off" />                                    <i class="icon-search nav-search-icon"></i>                                </span>                            </form>                        </div><!-- #nav-search -->                        {/if}                    </div>                    <div class="page-content">                        <div class="page-header">                            <h1>{$modul[$smarty.get.page].nazev}{if $modul[$smarty.get.page].sub[$smarty.get.subpage].nadpis}<small><i class="fa fa-angle-double-right"></i> {$modul[$smarty.get.page].sub[$smarty.get.subpage].nadpis}</small>{/if}</h1>                        </div>                        <div class="row">                            <div class="col-xs-12">                                <!-- PAGE CONTENT BEGINS -->                                {if $page.main.tpl}                                    {include $page.main.tpl}                                {/if}                                <!-- PAGE CONTENT ENDS -->                            </div><!-- /.col -->                        </div><!-- /.row -->                    </div><!-- /.page-content -->                </div><!-- /.main-content -->                {*include "./includes/ace-settings-container.tpl"*}            </div><!-- /.main-container-inner -->            <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">                <i class="fa fa-angle-double-up icon-only bigger-110"></i>            </a>        </div><!-- /.main-container -->        <script type="text/javascript">            if ("ontouchend" in document)                document.write("<script src='{$setup.adm.www}assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");        </script>        <script src="{$setup.adm.www}assets/js/bootstrap.min.js"></script>        <script src="{$setup.adm.www}assets/js/typeahead-bs2.min.js"></script>        <script src="{$setup.adm.www}assets/js/jquery.autosize.min.js"></script>        <script src="{$setup.adm.www}assets/js/jquery.colorbox-min.js"></script>        <script src="{$setup.adm.www}assets/js/date-time/bootstrap-datepicker.min.js"></script>        <script src="{$setup.adm.www}assets/js/date-time/locales/bootstrap-datepicker.cs.js"></script>        <!-- ace scripts -->        <script src="{$setup.adm.www}assets/js/ace-elements.min.js"></script>        <script src="{$setup.adm.www}assets/js/ace.min.js"></script>        <!-- inline scripts related to this page -->        {include "./includes/del_js.tpl"}        <script type="text/javascript">            $(function() {                {if $navSearch.show}                    //custom autocomplete (category selection)                    $.widget("custom.catcomplete", $.ui.autocomplete, {                        _renderMenu: function(ul, items) {                            var that = this,                                    currentCategory = "";                            $.each(items, function(index, item) {                                if (item.category != currentCategory) {                                    ul.append("<li class='ui-autocomplete-category'>" + item.category + "</li>");                                    currentCategory = item.category;                                }                                that._renderItemData(ul, item);                            });                        }                    });                    $("#search").catcomplete({                        delay: 0,                        minLength: 3,                        source: "{$setup.adm.request_url}",                        select: function( event, ui ) {                            window.location.href = ui.item.redirectUrl+ui.item.id;                        }                    });                    $("#search").focus(function(){                        $(".searchHighlight").prepend('<i class="icon-search blue" style="position:absolute; top:3px; right:3px;">');                        $(".searchHighlight .icon-search").effect("pulsate", 1500);                    });                    $("#search").focusout(function(){                        $(".searchHighlight .icon-search").remove();                    });                {/if}                $('.print').click(function() {                    window.print();                });                $('textarea[class*=autosize]').autosize({                    append: "\n"                });                $('.form-horizontal input[type=file]').ace_file_input({                    no_file: 'Není soubor ...',                    btn_choose: 'Vybrat',                    btn_change: 'Změnit',                    droppable: false,                    onchange: null,                    thumbnail: false,                    whitelist: 'xml'                })                $("[data-submitform]").click(function() {                    var form = $(this).data("submitform");                    $("#submitValue" + form).val($(this).data("submitvalue"));                    $("#form-" + form).submit();                });                $(".date-picker").datepicker({                    autoclose: true,                    showOtherMonths: true,                    selectOtherMonths: false,                    altField: "#actualDate",                    language: "cs",                    format: 'dd.mm.yyyy',                    weekStart: 1                });                var myDate = new Date();                var year = myDate.getFullYear();                var month = myDate.getMonth() + 1;                var date = myDate.getDate();                var prettyDate = date + '.' + month + '.' + year;                $(".date-picker-default").val(prettyDate);            });        </script>        {foreach $include.js AS $it}            {include $it}        {/foreach}    </body></html>